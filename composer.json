{
    "name": "robytepos/assets",
    "description": "Laravel assets management",
    "license": "MIT",
    "keywords": [
        "robytepos",
        "robytepos cms",
        "robytepos platform",
        "robytepos assets"
    ],
    "homepage": "https://robyte.az",
    "authors": [
        {
            "name": "Xalid Balagozov",
            "email": "xalidbalagozov@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "laravel/framework": "~5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1|^7.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Robytepos\\Assets\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Robytepos\\Assets\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Robytepos\\Assets\\Providers\\AssetsServiceProvider"
            ],
            "aliases": {
                "Assets": "Robytepos\\Assets\\Facades\\AssetsFacade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
